{
  "name": "@agusmgarcia/agustin-garcia",
  "private": true,
  "description": "Personal web site to show the technologies I have worked for and the technologies I use",
  "scripts": {
    "start": "next dev",
    "build": "del .next out && next build && next export && del out/assets",
    "lint": "next lint && prettier --check . && tsc --pretty --noEmit",
    "format": "next lint -- --fix && prettier --write ."
  },
  "dependencies": {
    "@agusmgarcia/swr": "2.0.1",
    "@fontsource/roboto": "4.5.5",
    "lazysizes": "5.3.2",
    "next": "12.1.0",
    "react": "17.0.2",
    "react-imgix": "9.3.0",
    "react-toastify": "7.0.4"
  },
  "devDependencies": {
    "@agusmgarcia/eslint-config": "1.0.1",
    "@types/node": "17.0.23",
    "@types/react": "17.0.39",
    "@types/react-imgix": "9.0.5",
    "del-cli": "5.0.0",
    "husky": "4.0.0",
    "prettier": "2.4.1",
    "sass": "1.37.5",
    "typescript": "4.4.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format"
    }
  },
  "engines": {
    "node": ">=16",
    "npm": ">=7"
  }
}
